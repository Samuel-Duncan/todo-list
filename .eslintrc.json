{
  "root": true,
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-alert": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": true,
        "printWidth": 80,
        "tabWidth": 2,
        "endOfLine": "auto"
      }
    ]
  }
}


